#include<iostream>
#include<stdlib.h>
//la librería stdlib.h sirve para poder utilizar rand
//matriz en la que los numeros son aleatorios
int main(){
	int matriz[3][3];
	int fil, col, num, salir;
	for (fil=0;fil<3;fil++){
		for(col=0;col<3;col++){
			//sacar un numero aleatorio
			//rand es la función para el numero aleaorio
			num=rand()%9; //el resto 9 serían numeros entre 1 y 9
			// los numeros que sacara estaran entre esos
			std::cout<<num<<" ";
			//matriz[fil][col]=num;
		}
	}
	std::cin>>salir;
	return 0;
}
